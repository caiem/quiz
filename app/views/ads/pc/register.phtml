                <div class="mask J-picMask"><div class="delete J-picDelete"></div></div>
            </li>
            <li>
                <span class="tag">点击添加</span>
                <input type="file" name="qualify[{{pos}}][{{count+5}}]" class="upload-btn J-upload" accept="image/gif,image/jpeg,image/jpg,image/png">
                <img class="upload-pics" src="{{ qualify && qualify.qualify_file && qualify.qualify_file[count+4] ? '/upload/'+ qualify.qualify_file[count+4] :''}}" />
                <div class="mask J-picMask"><div class="delete J-picDelete"></div></div>
            </li>
            {{ if qualify && qualify.qualify_file }}
                {{each qualify.qualify_file as value index}}
                    {{ if index>4 }}
                        {{if index%5 == 0 }}
                            </ul>
                        <ul class="upload-list clearfix">
                        {{/if}}
                        <li>
                            <span class="tag">点击添加</span>
                            <input type="file" name="qualify[{{pos}}][{{index+1}}]" class="upload-btn J-upload" accept="image/gif,image/jpeg,image/jpg,image/png">
                            <img class="upload-pics" src="{{ qualify && qualify.qualify_file && qualify.qualify_file[index] ? '/upload/'+ qualify.qualify_file[index] :''}}" />
                            <div class="mask J-picMask"><div class="delete J-picDelete"></div></div>
                        </li>
                    {{/if}}
                {{/each}}
                {{each qualify.qualify_file as value index }}
                    {{ if 5 - qualify.qualify_file.length%5 - index > 0 }}
                    <li>
                        <span class="tag">点击添加</span>
                        <input type="file" name="qualify[{{pos}}][{{qualify.qualify_file.length+index+1}}]" class="upload-btn J-upload" accept="image/gif,image/jpeg,image/jpg,image/png">
                        <img class="upload-pics" src="" />
                        <div class="mask J-picMask"><div class="delete J-picDelete"></div></div>
                    </li>
                    {{/if}}
                {{/each}}
            {{/if}}
        </ul>
    </script>
    <script src="/js/register.js?v=1.2"></script>
</body>
</html>
